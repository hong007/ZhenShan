            //    return '<a class="view">查看</a>| <a class="delete">删除</a>'
            //}}
        ];
        var mmg = $('.mmg').mmGrid({
            height: 800,
            width: 1170,
            cols: cols,
            url: '/sell/api.do',
            method: 'post',
            nowrap:true,
            remoteSort: true,
            root: 'result',
            fullWidthRows: true,
            loadingText: '云地网提醒您,数据正在努力加载中...',
            loadErrorText: '云地网提醒您,数据加载异常...',
            autoLoad: true,
            indexCol: true,
            indexColWidth: 30,
            plugins: [
                $('#pg').mmPaginator()
            ],
            params: function () {
                return {
                    method: 'api.sell.result.find',
                    data:$('#query').data("data")
                }
            }
        });

        $("#query").click(function () {
            //拼装查询结果
            var keyword = $('#mykeyword').val();
            var data = {};
            data.keyword = keyword;
            data.selectvalue = selectResult();
            $('#query').data("data",JSON.stringify(data));
            mmg.load({data: JSON.stringify(data)});

////            console.log(data);
////
////            data.push({keyword:keyword});
//            $.ajax({ type: "post",//使用get方法访问后台
//                dataType: "json",//返回json格式的数据
//                url:'/sell/api.do',
//                data:{method:'api.sell.result.find',data:data},
//                success: function(msg){
//                    console.log(msg);
//                }
//            })
        });
    });
