            $(this).addClass("more_bg");
            $(".more-region").show();
        }, function () {
            $(this).removeClass("more_bg");
            $(".more-region").hide();
        });

        $("#selectList").find("#purposeMore").toggle(function () {
            $(this).addClass("more_bg");
            $(".more-purpose").show();
        }, function () {
            $(this).removeClass("more_bg");
            $(".more-purpose").hide();
        });

        $("#selectList").find("#sellMethodMore").toggle(function () {
            $(this).addClass("more_bg");
            $(".more-sellMethod").show();
