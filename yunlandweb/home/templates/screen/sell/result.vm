            $(this).removeClass("more_bg");
            $(".more-sellMethod").hide();
        });

        var taboy_box = $(".lefttable-list");
        taboy_box.children("tbody").find("tr:gt(2)").hide();
        $(".leftbox-morea").toggle(function () {
                    $(this).parent().prev().find("tr").show();
                    $(this).addClass("more-i")
                }, function () {
                    $(this).removeClass("more-i");
                    $(this).parent().prev().children("tbody").find("tr:gt(2)").hide();
                }
        );
        var getsellmodel = function (modelvalue) {
            if (!modelvalue) {
                return "";
            }
            var sellmodels = [
                #foreach($param in ${selectSupplyMethod.keySet()})
                    {key: '$param', value: '${selectSupplyMethod.get($param)}'},
                #end
            ]
        ##
            for (var i = 0; i < sellmodels.length; i++) {
                var sellmodel = sellmodels[i];
                if (sellmodel && sellmodel.key == modelvalue) {
                    return "" + sellmodel.value;
                }
            }
        }

        var getPlanUse = function (value) {
            if (!value) {
                return "";
            }
            // 查询用途
            var planuses = [
                #foreach($param in ${selectLandPurpose.keySet()})
                    {key: '$param', value: '${selectLandPurpose.get($param)}'},
                #end
            ]
            for (var i = 0; i < planuses.length; i++) {
                var planuse = planuses[i];
                if (planuse && planuse.key == value) {
                    return "" + planuse.value;
                }
            }
        }

        var cols = [
            {
                title: '地块名称', name: 'name', align: 'center', renderer: function (val, item) {
                return '<a href="resultdetail?id=' + item.id + '" target="_blank" title='+val+'>' + val + '</a>'
            }
            },
            {title: '位置', name: 'address', align: 'center',renderer:function(val){
                return '<span title='+val+'>'+val+'</span>';
            }},
            {title: '面积(万平方米)', name: 'totalArea',align: 'center'},
            {
                title: '用途', name: 'planUse',align: 'center', renderer: function (val) {
                var value = getPlanUse(val);
                return '<span title='+value+'>'+value+'</span>';
            }
            },
            {
                title: '出让方式', name: 'supplyType', width:'10px', align:'center', renderer: function (val) {
                var value = getsellmodel(val);
                return "<span title='"+value+"'>" + value + "</span>";
            }
            },
            //{ title:'合作方式', name:'cooperationMode' , align:'right',   type: 'number'},
            {title: '出让期限(年)', name: 'sellYear', type: 'number',align: 'center',renderer: function (val){
                return "<span title='"+val+"'>" + val + "</span>";
            }},
            {title: '土地使用人', name: 'user',align: 'center'},
            {title: '成交总价(万元)', name: 'money', align: 'center', type: 'number'}
            //{ title:'状态', name:'status' ,align:'right', type: 'number'},
