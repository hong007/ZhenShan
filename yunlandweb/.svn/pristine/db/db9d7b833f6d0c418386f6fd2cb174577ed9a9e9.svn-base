}
.land-intro-verticleList .land-data .single-land .single-inner-middle1 .tab {
	border: 1px solid #04A4F8;
	height: 30px;
	line-height: 30px;
	display: inline-block;
	padding-left: 10px;
	padding-right: 10px;
	color: #04A4F8;
	font-size: 14px;
	margin-right: 10px;
	float: left;
}
.land-intro-verticleList .land-data .single-land .single-right .discussway2{
	margin-top: 10px;
	font-size: 16px;
	font-weight: 600;
	color: #ff7a29;
}
.data-renewal-title{
	height: 32px;
	margin: 10px 0 14px;
	border-bottom: 2px solid #ff7a29;
}
.data-renewal-title .title{
	float: left;
	height: 32px;
	padding: 0 20px;
	line-height: 32px;
	font-size: 20px;
	color: #fff;
	background: #ff7a29;
}
.data-renewal-title .more a{
