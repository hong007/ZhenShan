	margin-left: 0px;
	margin-top: 0px;
}
.land-intro-verticleList .land-data .single-land .single-inner-middle1 .title2 {
	font-size: 18px;
	color: #333333;
	margin-bottom: 15px;
}
.land-intro-verticleList .land-data .single-land .single-inner-middle1 .place-area {
	color: #333333;
	font-size: 14px;
	height: 30px;
	margin-bottom: 5px;
}
.land-intro-verticleList .land-data .single-land .single-inner-middle1 .place-area .icon-location {
	display: inline-block;
	background: url("../images/location.png") center top no-repeat;
