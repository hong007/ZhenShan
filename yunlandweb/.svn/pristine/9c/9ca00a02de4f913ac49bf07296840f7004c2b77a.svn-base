/*xnTableSelect begin*/
.xn_select_outer {
    width: 100%;
    clear: both;
    position: relative;
}
.xn_select_outer_div{
    width: 100%;
    height: 34px;
}
.xn_select_outer_div .xn_select_input{
    width: 100%;
    float: left;
    line-height: normal;
    height: 34px;
    border-radius: 4px;
    border: solid 1px #cdcdcd;
    padding: 6px 32px 6px 12px;
}
.xn_select_outer .xn_show_div{
    position: absolute;
    z-index: 100;
    background: white;
    width: 100%;
    line-height :normal;
    border: solid 1px #cdcdcd;
    border-radius: 4px;
    box-shadow: 0 0 20px #cdcdcd;
    min-height: 330px;
}
.xn_select_outer .xn_show_div .content {
    display: table;
    width: 100%;
    height: 280px;
    overflow-y: auto;
}
.xn_select_outer .xn_show_div .content .pagination {
    position: absolute;
    right: 0;
    top: 75px;
    z-index: 99;
}
.xn_select_outer .xn_show_div .content .pagination .icon {
    border: 1px solid #cdcdcd;
    text-align: center;
    line-height: 26px;
    width: 32px;
    height: 32px;
    border-radius: 16px;
    background: #ececec;
    opacity: 0.5;
    cursor: pointer;
}
.xn_select_outer .xn_show_div .content .pagination .icon-up {
    margin-bottom: 30px;
}
.xn_select_outer .xn_show_div .content .pagination .icon-up-disable {
    color: #FCFCFC;
    cursor: default;
}
.xn_select_outer .xn_show_div .content .pagination .icon-down-disable {
    color: #FCFCFC;
    cursor: default;
}
.xn_select_outer .xn_show_div .content .sort_area {
    display: table-cell;
    border-right: solid 1px #cdcdcd;
    width: 35px;
    background: #f9f9f7;
}
.xn_select_outer .xn-search{
    padding: 5px;
    margin: 0;
    width: 350px;
}
.xn_select_outer .xn-search .input-group input{
    border:none;
    border-top-right-radius: 5px;
    border-bottom-right-radius: 5px;
}
.xn_select_outer .xn-search .input-group-addon {
    border:none;
}
.xn_select_outer table{
    width: 100%;
}
.xn_select_outer .xn_show_div i {
    float: right;
    color:#cdcdcd;
}
.xn_select_outer .xn_show_div .icon-delete:hover {
    color: #ff0000;
    cursor: pointer;
}
.xn_select_outer .xn_show_div .xn-search{
    float:left;
}
.xn_select_outer .xn_select_input_div {
    width: 100%;
    border-radius: 4px;
    float: left;
    cursor: pointer;
}
.xn_select_outer .xn_select_input_div .icon-delete {
    position: absolute;
    right: 35px;
    margin-top: -2px;
    color: #cdcdcd;
    line-height: 34px;
}
.xn_select_outer .xn_select_input_div .icon-delete:hover{
    color: #ff0000;
    cursor: pointer;
}
.xn_select_outer .xn_select_input_div .icon-apostrophe{
    position: absolute;
    margin-left: -34px;
    background: #eee;
    line-height: 28px;
    width: 33px;
    border-bottom-right-radius: 4px;
    border-top-right-radius: 4px;
    text-align: center;
    margin-top: 1px;
    padding-bottom: 4px;
    color: #555;
}
.xn_select_outer .pagination {
    margin-right:5px;
}
.xn_select_outer .xn_show_div .search_div {
    padding: 5px;
    float: left;
    width: 100%;
    background: #f9f9f7;
}
.xn_select_outer .xn_show_div .search_div .search_input_div {
    padding: 2px 0;
    float: left;
    display: table;
    width: 100%;
    border-collapse: separate;
}
.xn_select_outer .xn_show_div .search_div .search_input_div input{
    width: 90%;
    float: left;
    height: 30px;
    border-radius: inherit;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}
.xn_select_outer .xn_show_div .search_div .search_input_div .searc_icon_search {
    float: left;
    cursor:pointer;
    display: table-cell;
    width: 10%;
    border: solid 1px #cdcdcd;
    border-left: none;
    border-top-right-radius: 4px;
    border-bottom-right-radius: 4px;
    background: #eee;
    line-height: 20px;
    text-align: center;
    font-size: 20px;
    padding-bottom: 8px;
    color:#555;
}
.xn_select_outer .xn_show_div .transclude_div {
    width: 100%;
    min-height: 300px;
    position: relative;
}
.xn_select_outer .xn_show_div .transclude_div table {
    border-collapse: separate;
}
.xn_select_outer .xn_show_div .transclude_div table thead tr{
    border-bottom: solid 1px #ebebeb;
}
.xn_select_outer .xn_show_div .transclude_div table tbody tr{
    cursor:pointer;
    border: none;
    height: auto;
}
.xn_select_outer .xn_show_div .transclude_div table tbody tr:hover{
    background: #ebebeb;
}
.xn_select_outer .xn_show_div .transclude_div table tr td{
    line-height: 28px;
    border: none;
    text-align: left;
}
.xn_select_outer .xn_show_div .pagination-sm {
    clear: both;
    margin: 5px;
}
/*xnTableSelect end*/

