/*打钩样式*/
.xn-label-checkbox{
  position: relative;
  line-height: 34px;
  cursor: pointer;
}
.xn-label-checkbox .xn-checkbox{
  width: 20px;
  height: 20px;
  border: 1px solid #ccc;
  display: inline-block;
  margin-top: 7px ;
  margin-right: 7px;
  .border-radius();
  float: left;
}
.xn-label-checkbox .icon-right_3{
  position: absolute;
  left: 5px;
  z-index: 1;
  font-size: 18px;
  height: 20px;
  width: 20px;
  top: 5px;
  line-height: 20px;
  color: #40a337;
}

.xn-label-checkbox .xn-checkbox-input{
  display: none;
}
