/* 土地招拍挂 */
.promote-con-left .double-right{
	margin-left: 17px;
	float: left;
	width: 74.33%;
}
.promote-con-left .double-right .title{
	width: 85%;
}
.promote-con-left .double-right .middle{
	margin-top: 8px;
	text-align: right;
	line-height: 28px;
	overflow: hidden;
}
.promote-con-left .double-right .middle .middle-left{float:left;}
.promote-con-left .double-right .middle .middle-right{float:right;}
.promote-con-left .double-right .middle .middle-left span{
	float: left;
	padding: 0 10px;
	height: 28px;
	line-height: 28px;
	background: #e8f2f2;
	border-radius: 3px;
	text-align: center;
	color: #0e76d5;
}
.promote-con-left .double-right .middle .middle-left .coding{
	padding: 0;
	background-color: transparent;
}
.promote-con-left .double-right .middle .middle-right span{
	color: #666;
}
.promote-con-left .double-right .single-inner-middle .place-area{
	float: left;
}
.promote-con-left .double-right .single-inner-middle .place-area2{
	float: right;
	margin-left: 30px;
	height: 24px;
	line-height: 24px;
	color: #666;
	font-size: 12px;
}
.promote-con-left .double-right .single-inner-middle .place-area span{
	margin-right: 8px;
}
.promote-con-left .double-right .single-inner-middle .place-area .icon-location{
	display: inline-block;
    background: url("../images/location.png") center top no-repeat;
    margin-top: 2px;
    width: 12px;
    height: 16px;
    float: left;
    margin-right: 10px;
}
/* 土地招拍挂 end */
/*分页*/
div.sabrosus{padding:3px;margin:3px;text-align:center; font-size:16px;}
div.sabrosus a{border:#9aafe5 1px solid;padding:2px 5px;margin:0 2px 0 0;text-decoration:none;}
div.sabrosus a:hover{border:#2b66a5 1px solid;color:#000;background-color:lightyellow;}
div.pagination a:active{border:#2b66a5 1px solid;color:#000;background-color:lightyellow;}
div.sabrosus span.current{border:navy 1px solid;padding:2px 5px;font-weight:bold;color:#fff;margin:0 2px 0 0;background-color:#2e6ab1}
div.sabrosus span.disabled{border:#929292 1px solid;padding:2px 5px;margin:0 2px 0 0;}

