.xq:hover{color: #c35512; text-decoration:underline;}
.docking-list .list-con .text-con{
	width: 100%;
	line-height: 24px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.docking-list .list-con ul li{
	height: 80px;
	margin-bottom: 10px;
	font-size: 12px;
}
.docking-list .list-con .date{
	color: #666;
}
.docking-list .docking-weixin{
	height: 90px;
	margin-top: 10px;
	padding: 10px;
	border: solid 1px #dcdcdc;
}
.docking-list .docking-weixin .scan{
	height: 34px;
	line-height: 34px;
	padding-left: 50px;
	margin: 5px 0;
	font-size: 18px;
	color: #ff7a29;
	background: url(../images/weixin-icon.png) no-repeat center left;
}
/* 园区招商 */
.promote-con-right .list-con .youx li{
	float: left;
	margin-bottom: 10px;
}

/* 云地资讯 */
.zixun-list .list-con ul li{
	height: 74px;
	margin-top: 10px;
}
.promote-con-right .list-con ul li .text-title{
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	font-size: 14px;
}
.zixun-list .list-con ul li .text-con{
	font-size: 12px;
	color: #666;
}
.zixun-list .list-con ul li .text-con span{
	color: #ff7a29;
}
.zixun-list .list-con ul li .text-title:hover a{
	text-decoration: underline;
}
.promote-con-right .list-con .hot-list li{
	height: 18px;
	line-height: 18px;
	margin-top: 10px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.promote-con-right .list-con .hot-list li a{
	margin-left: 10px;
}
.promote-con-right .list-con .hot-list li a:hover{
	text-decoration: underline;
}
.promote-con-right .list-con .hot-list li.focus a{
	color: #ff7a29;
}
.promote-con-right .list-con .hot-list li span{
	display: inline-block;
	width: 16px;
	height: 16px;
	line-height: 16px;
	text-align: center;
	background: #bfbfbf;
	color: #fff;
}
.promote-con-right .list-con .hot-list li.focus span{
	background: #ff7a29;
}
/* 城市更新 */
.promote-con-right .list-con .city-gaizao li{
	height: 70px;
	margin-bottom: 15px;
	font-size: 12px;
}
.promote-con-left .city-renewal-zx .focus_container{
	width:340px;
	height:214px;
	background:#dcdcdc;
	float:left;
	position:relative;
	font-size:12px;
	overflow:hidden;
}
.promote-con-left .data-right{
	float: left;
	margin-left: 10px;
	width: 57.5%;
}
.promote-con-left .data-right h3{
	padding-top: 6px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.promote-con-left .data-right h3:hover a{
	color: #e45f5d;
}
.promote-con-left .data-right p{
	line-height: 23px;
	color: #666;
}
.promote-con-left .data-right span{
	color: #ff7a29;
}
.promote-con-left .data-bottom{
	height: 128px;
	margin-top: 10px;
	border: 1px solid #e5e5e5;
}
.promote-con-left .data-bottom ul li{
	float: left;
	width: 260px;
	height: 42px; 
	line-height: 42px;
	padding-left: 10px;
	font-size: 16px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #0e76d5;
}
.promote-con-left .data-bottom ul li a{
	color: #0e76d5;
}
.promote-con-left .data-bottom ul li a:hover{
	text-decoration: underline;
	color: #e45f5d;
}
.promote-con-left .city-renewal-al ul li{
	float: left;
	margin-left: 6px;
	width: 190px;
	height: 252px;
	padding: 10px 4px;
	border: 1px solid #dcdcdc;
	overflow: hidden;
}
.promote-con-left .city-renewal-al ul li h3{
	text-align: center;
	font-size: 15px;
	background: #daefff;
}
.promote-con-left .city-renewal-al ul li img{
	display: block;
	margin: 12px 0 8px; 
}
.promote-con-left .city-renewal-al ul li p{
	line-height: 24px;
	color: #666;
}
.promote-con-left .city-renewal-al ul li p span{
	color: #ff7a29
}
.promote-con-left .city-renewal-al ul li p a{
	color: #ff7a29
}
.promote-con-left .city-renewal-al ul.data-bottom {
	border: none;
}
.promote-con-left .city-renewal-al ul.data-bottom li{
	width: 396px;
	height: 156px;
	margin-top: 10px;
}
.promote-con-left .city-renewal-al ul.data-bottom li img{
	float: left;
}
.promote-con-left .city-renewal-al ul.data-bottom li p{
	float: left;
	width: 195px;
	margin-top: 12px;
	margin-left: 10px;
}
.promote-con-left .city-renewal-fl .data-list{
	float: left;
	width: 383px;
	margin-left: 6px;
	margin-bottom: 10px;
	padding: 0 10px 16px;
	border: 1px solid #dcdcdc;
}
.promote-con-left .city-renewal-fl .data-list .city-fl-title{
	height: 40px;
	line-height: 40px;
	border-bottom: 1px solid #dcdcdc;
}
.promote-con-left .city-renewal-fl .data-list .city-fl-title .title{
	float: left;
	font-size: 16px;
	font-weight: bold;
}
.promote-con-left .city-renewal-fl .data-list .city-fl-title .more{
	float: right;
	margin-top: 7px;
	padding: 0 18px;
	height: 26px;
	line-height: 26px;
	border: 1px solid #dcdcdc;
	border-radius: 14px;
	-webkit-border-radius: 14px;
	-moz-border-radius: 14px;
}
.promote-con-left .city-renewal-fl .data-list .city-fl-title .more a{
	height: 26px;
	line-height: 26px;
	padding-right: 20px;
	background: url(../images/more-icon2.gif) no-repeat right center;
	font-size: 15px;
	color:  #949494;
}
.promote-con-left .city-renewal-fl .data-list ul li{
	height: 30px;
	line-height: 30px;
	text-align: left;
}
.promote-con-left .city-renewal-fl .data-list ul li a{
	display: inline-block;
	width: 250px;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap;
}
.promote-con-left .city-renewal-fl .data-list ul li:hover a{
	color: #ff7a29;
}
.promote-con-left .city-renewal-fl .data-list ul li:hover span{
	color: #ff7a29;
}
.promote-con-left .city-renewal-fl .data-list ul li span{
	float: right;
	color: #c9c9c9;
}
