.label-long .xn-label{
  width: 167px;
  margin-left: -1px;
}
.land-recommend-list {
  width: 100%;
}
.land-recommend-tab-title{
  width: 100%;
  height: 40px;
  line-height: 40px;
  border-bottom: 2px solid #ff7a29;
  font-size: 16px;
  position: relative;
}
.position-relative{
  position: relative;
}
.position-relative ul{
  position: absolute;
  left: 0;
  top: -1px;
  width: 100%;
}
.position-relative ul li{
  padding: 0 20px;
  border:1px solid #dcdcdc;
  float: left;
  border-bottom: none;
}
.position-relative .on{
  background: #ff7a29;
  color: #fff;
}
.land-recommend-tab-con{
  width: 100%;
  box-sizing: border-box;
  border: 1px solid #dcdcdc;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  height: 300px;
}

.land-detail{
  height: 100%;
}