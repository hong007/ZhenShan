//二级菜单
.xn-nav-2 .dropdown ul{
  height: auto;
}
.xn-nav-2 .dropdown-menu{
  position: absolute;
  top: 35px;
  left: 0;
  border: 1px solid rgba(0, 0, 0, 0.15);
  border-radius: 4px;
  display: block;
}
.xn-nav-2 .dropdown-menu li{
  float: none;
  height: 30px;
}
.xn-nav-2 .dropdown-menu li:nth-child(n+2){
  margin-left: 0px;
}
.xn-nav-2 ul>li.dropdown .dropdown-menu li a{
  display: block;
  cursor: pointer;
  color: #333;
  height: 30px;
  padding-left: 5px;
}

