/*xnMakesure*/
.delete_dialog_wrap {
    width: 300px;
    min-height: 160px;
    border-radius: 4px;
    box-shadow: 0 0 10px #cdcdcd;
    position: absolute;
    z-index: 5;
    background: #fff;
    user-select: none;
    -o-user-select: none;
    -moz-user-select: none;
    -webkit-user-select: none;
}

.delete_dialog_wrap .dialog_title {
    height: 35px;
    display: table;
    width: 100%;
    margin: 0;
    padding: 0;
    border-bottom: solid 1px #cdcdcd;
}

.delete_dialog_wrap .dialog_title li {
    display: table-cell;
}

.delete_dialog_wrap .dialog_title li:first-child {
    width: 95%;
    vertical-align: middle;
    padding: 0 10px;
}

.delete_dialog_wrap .dialog_title li:first-child>h4 {
    margin-top: 0;
    margin-bottom: 0;
}

.delete_dialog_wrap .dialog_title li:last-child {
    vertical-align: top;
    color: #cdcdcd;
    cursor: pointer;
}

.delete_dialog_wrap .dialog_title li:last-child:hover {
    color:#d9534f;
}

.delete_dialog_wrap .content_table {
    min-height: 85px;
    display: table;
    width: 100%;
    padding: 8px;
}

.delete_dialog_wrap .content_table .delete_dialog_content {
    display: table-cell;
    vertical-align: middle;
    text-align: center;
}

.delete_dialog_wrap .btn_wrap {
    display: table;
    width: 100%;
    padding: 0;
    height: 40px;
    margin-bottom: 0;
}

.delete_dialog_wrap .btn_wrap li {
    display: table-cell;
    text-align: center;
    list-style-type: none;
}

.delete_dialog_wrap .btn_wrap .btn_li {
    vertical-align: bottom;
}

.delete_dialog_wrap .btn_wrap li button {
    width: 80px;
}
