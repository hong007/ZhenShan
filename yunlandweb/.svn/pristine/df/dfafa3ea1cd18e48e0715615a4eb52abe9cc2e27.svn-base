/*quickSearchBox*/
.search_box {
    line-height: 19px;
    width: 50px;
    border: none;
    padding: 0 6px;
    outline: none;
}
.box_wrap {
    cursor: pointer;
    float: left;
    line-height: 28px;
    border-radius: 30px;
    padding: 0 10px 0 4px;
}
.box_wrap .icon-chaxun01{
    color: #fff;
    font-size: 12px;
}
.box_wrap_animation {
    animation: searchInputDiv 0.5s;
    -webkit-animation: searchInputDiv 0.5s;
    -moz-animation: searchInputDiv 0.5s;
    -o-animation: searchInputDiv 0.5s;
    animation-fill-mode : forwards;
    -webkit-animation-fill-mode : forwards;
    -moz-animation-fill-mode : forwards;
    -o-animation-fill-mode : forwards;
    animation-timing-function : linear;
    -webkit-animation-timing-function : linear;
    -o-animation-timing-function : linear;
    -moz-animation-timing-function : linear;
}
.box_wrap_animation .search_box {
    animation: searchInput 0.5s;
    -webkit-animation: searchInput 0.5s;
    -moz-animation: searchInput 0.5s;
    -o-animation: searchInput 0.5s;
    animation-fill-mode : forwards;
    -webkit-animation-fill-mode : forwards;
    -moz-animation-fill-mode : forwards;
    -o-animation-fill-mode : forwards;
    animation-timing-function : linear;
    -webkit-animation-timing-function : linear;
    -o-animation-timing-function : linear;
    -moz-animation-timing-function : linear;
}
.box_wrap_animation .icon-chaxun01 {
    float: right;
    animation: searchIcon 0.5s;
    -webkit-animation: searchIcon 0.5s;
    -moz-animation: searchIcon 0.5s;
    -o-animation: searchIcon 0.5s;
    animation-fill-mode : forwards;
    -webkit-animation-fill-mode : forwards;
    -moz-animation-fill-mode : forwards;
    -o-animation-fill-mode : forwards;
    animation-timing-function : linear;
    -webkit-animation-timing-function : linear;
    -o-animation-timing-function : linear;
    -moz-animation-timing-function : linear;
}
@keyframes searchInput{
    to {
        width: 120px;
        background: #fff;
    }
}
@-webkit-keyframes searchInput{
    to {
        width: 120px;
        background: #fff;
    }
}
@-moz-keyframes searchInput{
    to {
        width: 120px;
        background: #fff;
    }
}
@-o-keyframes searchInput{
    to {
        width: 120px;
        background: #fff;
    }
}
@keyframes searchInputDiv {
    to {
        background: #fff;
    }
}
@-webkit-keyframes searchInputDiv {
    to {
        background: #fff;
    }
}
@-moz-keyframes searchInputDiv {
    to {
        background: #fff;
    }
}
@-o-keyframes searchInputDiv {
    to {
        background: #fff;
    }
}
@keyframes searchIcon {
    to {
        color:#cdcdcd;
    }
}
@-webkit-keyframes searchIcon {
    to {
        color:#cdcdcd;
    }
}
@-moz-keyframes searchIcon {
    to {
        color:#cdcdcd;
    }
}
@-o-keyframes searchIcon {
    to {
        color:#cdcdcd;
    }
}
