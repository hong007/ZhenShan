/*logo*/
.logo_box_wrap {
    width: 100%;
    height: 50px;
    padding-left: 20px;
    float:left;
}

.logo_box_wrap .logo_forward .logo_title {
    width: 100%;
    height: 100%;
}

.logo_box_wrap .app_widget {
    position: absolute;
    max-width: 480px;
    z-index: 999;
    padding-top: 15px;
    margin-top: -10px;
}
.logo_box_wrap .app_widget .arrow_div {
    border: 8px solid #fff;
    width: 0;
    height: 0;
    transform: rotate(45deg);
    margin-left: 25px;
    box-shadow: -2px -2px 1px #cdcdcd;
    z-index: 9999;
}

.logo_box_wrap .app_widget .widget_inner {
    float: left;
    background: #fff;
    margin-top: -10px;
    box-shadow: 0 5px 15px rgba(0,0,0,.5);
    padding: 0 0 10px 0;
    border-radius: 4px;
}

.logo_box_wrap .app_widget .widget_inner .app_ul {
    -webkit-margin-before: 0;
    -webkit-margin-after: 0;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    -webkit-padding-start: 0;
    list-style-type: none;
    float: left;
    width: 100%;
    height: 100%;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li {
    float: left;
    width: 102px;
    height: 100px;
    margin-left: 15px;
    margin-top: 15px;
    cursor: pointer;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li a {
    text-decoration: none;
    padding: 0;
    width: 100%;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .img_div {
    width: 64px;
    height: 64px;
    margin-left: 19px;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .img_div:last-child {
    border-radius: 10px;
    border: solid 1px #cdcdcd;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .img_div img {
    border-radius: 10px;
    -webkit-filter: grayscale(70%);
    -moz-filter: grayscale(70%);
    -o-filter: grayscale(70%);
    filter: grayscale(70%);
    filter: gray;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .img_div img:hover {
    -webkit-filter: grayscale(0);
    -moz-filter: grayscale(0);
    -o-filter: grayscale(0);
    filter: grayscale(0);
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .label_div {
    width: 100%;
    text-align: center;
    color: #666;
    padding: 5px 0;
    line-height: 1;
    font-size: 14px;
}

.logo_box_wrap .app_widget .widget_inner .app_ul .app_li .img_div .more_app {
    font-size: 20px;
    text-align: center;
    color: #cdcdcd;
    width: 50px;
    margin-left: 6px;
    margin-top: 2px;
    line-height: 1.5;
}
