/*返回顶部*/
.xn-back-to-top{
    position: fixed;
    right: 10px;
    height: auto;
    bottom: 10px;
    width: 52px;
}
.xn-back-to-top ul{
    text-align: center;

    width: 52px;
    border-top: 1px solid #dfdfdf;
    border-left: 1px solid #dfdfdf;
    border-right: 1px solid #dfdfdf;
}
.xn-back-to-top ul li{
    width: 50px;
    border-bottom: 1px solid #dfdfdf;
}
.xn-back-to-top ul li a{
    width: 50px;
    padding: 5px;
    display: inline-block;
    background:#fff;
    color: #999;
}
.xn-back-to-top ul li a:hover{
    text-decoration: none;
    background:#fff;

}
.xn-back-to-top ul li a .gotop-icon{
    display: inline-block;
    font-size: 20px;
}
.xn-back-to-top ul li a .gotop-font{
    display: none;
    font-size: 12px;
    line-height: 20px;
}
.xn-back-to-top ul li a:hover .gotop-icon{
    display: none;
}
.xn-back-to-top ul li a:hover .gotop-font{
    display: inline-block;
    font-size: 12px;
    line-height: 20px;
}
.xn-back-to-top ul li a .feedback-icon{
    display: inline-block;
    font-size: 20px;
}
.xn-back-to-top ul li a .feedback-font{
    display: none;
}
.xn-back-to-top ul li a:hover .feedback-icon{
    display: none;
}
.xn-back-to-top ul li a:hover .feedback-font{
    display: inline-block;
}
/*xnTagInput*/
.xnInput_out_div {
    border: solid 1px #cdcdcd;
    line-height: 34px;
    min-height: 34px;
    border-radius: 4px;
    float: left;
    width: 100%;
}
.xnInput_out_div .xnInput_ul {
    list-style: none;
    -webkit-margin-before: 0em;
    -webkit-margin-after: 0;
    -webkit-margin-start: 0px;
    -webkit-margin-end: 0px;
    -webkit-padding-start: 0px;
    float: left;
}
.xnInput_out_div .xnInput_ul>li{
    float: left;
    margin-left:12px;
}
.xnInput_out_div .xnInput_ul .con_li{
    line-height: 20px;
    margin-top: 5px;
    padding-bottom: 3px;
    background: #ebebeb;
    border-radius: 4px;
}
.xnInput_out_div .xnInput_ul>li>div {
    float: left;
}
.xnInput_out_div .xnInput_ul>li>a{
    margin-left: 10px;
}
.xnInput_out_div .xnInput_ul>li .xnInput_input {
    line-height: 32px;
    outline: none;
    border: none;
}
.input_outline {
    border-color: rgba(82, 168, 236, 0.8);
    outline: 0;
    outline: thin dotted \9;
    -webkit-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);
    -moz-box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);
    box-shadow: inset 0 1px 1px rgba(0,0,0,.075), 0 0 8px rgba(82,168,236,.6);
}
.xnInput_out_div .xnInput_ul .li_a .icon-delete {
    color: #333;
}
.xnInput_out_div .xnInput_ul .li_a .icon-delete:hover {
    color: #ff0000;
    cursor: pointer;
}
.xnInput_out_div .xnInput_ul .li_a{
    text-decoration: none;
}
