/*xnMessageSlide*/
.xnMessage {
    border-radius: 4px;
    background: #fff;
    width: 0;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: clip;
    height: 0;
    float:left;
}

.xnMessage_success {
    margin-top: 10px;
    height: auto;
    padding: 10px;
    width: auto;
    box-shadow:inset 0 0 10px #008B45;
    min-width: 200px;
}

.xnMessage_error {
    margin-top: 10px;
    height: auto;
    padding: 10px;
    width: auto;
    box-shadow:inset 0 0 10px #F08080;
    min-width: 200px;
}
.xnMessage_null {
    margin-top: 10px;
    height: auto;
    padding: 10px;
    width: auto;
    box-shadow:inset 0 0 10px #F08080;
    min-width: 200px;
}

.xnMessage_warning {
    margin-top: 10px;
    height: auto;
    padding: 10px;
    width: auto;
    box-shadow:inset 0 0 10px #FFC125;
    min-width: 200px;
}

.xnMessage .content {
    font-size: 14px;
    color: #999;
}

.xnMessage .content .title {
    color: #666;
    font-weight: bold;
}
