                            {key:'6',value:'债权融资'},
                            {key:'7',value:'租赁融资'},
                    ]
        for(var i=0;i<cooperationModes.length;i++){
            var cooperationMode = cooperationModes[i];
            if(cooperationMode&&cooperationMode.key==value){
